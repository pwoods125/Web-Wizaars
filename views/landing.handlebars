<div>
    {{>primary}}
</div>
<div class="profileDisplay-container">
    <div class="card-header col-md-12 text-center #eceff1 blue-grey-text text-lighten-5">
        <h3>
            Your Profile
        </h3>
    </div>

    <div>
        <ul class="collection with-header">
            <li class="collection-header">
                <h4>{{this.firstname}} {{this.lastname}}</h4>
            </li>
            <li class="collection-item">About me: {{this.aboutme}}</li>
            <li class="collection-item">PSN: {{this.usernamepsn}}</li>
            <li class="collection-item">Xbox Gamertag: {{this.usernamexbox}}</li>
            <li class="collection-item">Steam name: {{this.usernamesteam}}</li>
            <li class="collection-item">Nintendo tag: {{this.usernamenintendo}}</li>
        </ul>
    </div>

</div>


<script src="/js/logout.js"></script>